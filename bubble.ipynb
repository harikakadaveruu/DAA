   "source": [
    "def bubble_sort(arr):\n",
    "    n=len(arr)\n",
    "    for i in range(n):\n",
    "        for j in range(0,n-i-1):\n",
    "            if arr[j]>arr[j+1]:\n",
    "                arr[j],arr[j+1]=arr[j+1],arr[j]\n",
    "l=[7,9,27,19,10,5]\n",
    "bubble_sort(l)\n",
    "print(\"Sorted array->\",l)"
   ]
