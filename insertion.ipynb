   "source": [
    "def insertion_sort(arr):\n",
    "    for i in range(1,len(arr)):\n",
    "        key=arr[i]\n",
    "        j=i-1\n",
    "        while j>=0 and key<arr[j]:\n",
    "            arr[j+1]=arr[j]\n",
    "            j-= 1\n",
    "        arr[j+1]=key\n",
    "l=[2,5,8,10,9]\n",
    "insertion_sort(l)\n",
    "print(\"Sorted array->\",l)\n",
    "\n"
   ]
