    "def selection_sort(arr):\n",
    "    n=len(arr)\n",
    "    for i in range(n-1):\n",
    "        min_i=i\n",
    "        for j in range(i+1,n):\n",
    "            if arr[j]<arr[min_i]:\n",
    "                min_i=j\n",
    "        arr[i],arr[min_i]=arr[min_i],arr[i]\n",
    "l = [3,12,20,1,14]\n",
    "selection_sort(l)\n",
    "print(\"Sorted array->\",l)"
   ]
